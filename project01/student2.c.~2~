#include <stdio.h>
#include <assert.h>
#include "functions.h"

int main(void) {
  assert(ith_factor(1,1,1) == 1);
  assert(ith_factor(624,-48,7) == 12);
  assert(ith_factor(624,48,7) == 12);
  assert(ith_factor(624,48,0) == -1);
  assert(ith_factor(100,10,-1) == -1);
  assert(ith_factor(0,10,1) == -1);
  assert(ith_factor(10,0,1) == -1);


  printf("\nThis test has passed!\n");

  return 0;
}
